{
  "name": "@ckt1031/eslint-config",
  "version": "0.0.13",
  "description": "ESLint config for ckt1031, ensure the consistency of code quality.",
  "scripts": {
    "build": "tsup src --format cjs,esm --minify",
    "lint": "tsc"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckt1031/nodejs-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "ckt1031"
  ],
  "author": "Chan Ka Tsun <me@cktidy.xyz> (https://github.com/ckt1031/)",
  "license": "MIT",
  "homepage": "https://github.com/ckt1031/nodejs-config#readme",
  "bugs": {
    "url": "https://github.com/ckt1031/nodejs-config/issues"
  },
  "devDependencies": {
    "@ckt1031/tsconfig": "workspace:*",
    "@types/deepmerge": "2.2.0",
    "@types/eslint": "8.21.0",
    "eslint": "8.33.0",
    "tsup": "6.5.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "eslint": "8.x"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "deepmerge": "4.3.0",
    "eslint-plugin-autofix": "1.1.0",
    "eslint-plugin-deprecation": "1.3.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-regexp": "1.12.0",
    "eslint-plugin-security": "1.7.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-tailwindcss": "3.8.3",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint-plugin-write-good-comments": "0.1.4"
  },
  "exports": {
    ".": {
      "import": "./dist/default.mjs",
      "require": "./dist/default.js"
    },
    "./default": {
      "import": "./dist/default.mjs",
      "require": "./dist/default.js"
    },
    "./typescript-strict": {
      "import": "./dist/typescript-strict.mjs",
      "require": "./dist/typescript-strict.js"
    }
  }
}
